<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.BoardMapper">
    <select id="selectBoard" parameterType="java.lang.String" resultType="Board">
        select * from board where author = #{author}
